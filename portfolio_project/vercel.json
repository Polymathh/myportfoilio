{
  "version": 2,
  "builds": [
    {
      "src": "portfolio_project/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "staticfiles/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "portfolio_project/wsgi.py"
    }
  ]
}
